{
    "name": "su-sws/acsf-cardinalsites-public",
    "description": "Cardinalsites ACSF Stack. D8.",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://userguide.sites.stanford.edu/",
    "support": {
        "docs": "https://userguide.sites.stanford.edu/"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/a11ychecker",
                "version": "1.1.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/a11ychecker/releases/a11ychecker_1.1.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/balloonpanel",
                "version": "4.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/balloonpanel/releases/balloonpanel_4.14.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/link",
                "version": "4.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/link/releases/link_4.14.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/fakeobjects",
                "version": "4.14.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.14.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/locales",
                "version": "1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/locales.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/styles-distribution",
                "version": "1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/styles-distribution.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "acquia/blt": "^13.4",
        "acquia/blt-acsf": "^1.0",
        "acquia/blt-simplesamlphp": "^1.0",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "drupal/acquia_purge": "^1.1",
        "drupal/acsf": "^2.68",
        "drupal/core": "^9.0",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9.0",
        "drupal/menu_block": "dev-1.x#17bc5a2094dec85a921fdac6aff0030bfe004744",
        "drupal/mysql56": "^1.1",
        "drush/drush": "^10.3",
        "su-sws/stanford_profile": "8.x-dev"
    },
    "require-dev": {
        "acquia/blt-drupal-test": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "su-sws/blt-sws": "dev-main",
        "su-sws/drupal-dev": "^8.2"
    },
    "replace": {
        "drupal/coder": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "gitignore": false
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "custom-installer": {
            "docroot/libraries/{$name}/": [
                "type:component",
                "ckeditor/ckeditor",
                "enyo/dropzone",
                "components/jquery",
                "harvesthq/chosen"
            ],
            "vendor/{$vendor}/{$name}/": [
                "type:library"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "https://www.drupal.org/project/drupal/issues/3151977": "patches/media-recursion-limit.patch"
            },
            "drupal/ui_patterns": {
                "Ui Patterns Views Fixes": "patches/contrib/ui_patterns_views-preview.patch"
            },
            "ckeditor-plugin/link": {
                "http://github.com/ckeditor/ckeditor4/pull/3965": "patches/libraries/ckeditor-plugin-link-anchor-id.patch",
                "Disable double click double modal": "patches/libraries/ckeditor-plugin-link-disable-double.patch"
            }
        }
    },
    "scripts": {
        "init-lando": [
            "cp lando/example.env .env",
            "cp lando/example.lando.yml .lando.yml",
            "cp lando/example.local.blt.yml blt/local.blt.yml",
            "cp lando/example.php.ini lando/php.ini",
            "blt blt:init:settings",
            "blt source:build:simplesamlphp-config"
        ]
    },
    "autoload": {
        "psr-4": {
            "CardinalSites\\": "blt/src/"
        }
    }
}
